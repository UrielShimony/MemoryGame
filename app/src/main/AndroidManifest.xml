<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.urielshimony.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UI.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ToActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".UI.choseDifficultActivity"
            android:label="@string/title_activity_chose_difficult"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UI.GameMediumLevel"
            android:label="@string/title_activity_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UI.GameActivity"></activity>
    </application>

</manifest>